<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.saipredictionalgorithm">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/bla"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bla"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".GraphActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RainFallActivity" />
        <activity android:name=".WeatherPrediction" />
        <activity android:name=".ValuesActivity"></activity>
    </application>

</manifest>